{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "SoyaMilk-design": ["src"],
      "SoyaMilk-design/*": ["src/*", "*"],
      "@": ["src"],
      "~@": ["src"],
      "@/*": ["src/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
